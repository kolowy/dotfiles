{
	"name": "vscodevimacademy",
	"displayName": "Vscode Vim Academy",
	"description": "A game to learn vim and vscode keys and build up your vim/vscode muscle memory",
	"version": "1.1.8",
	"author": "kaisun(julio),chou",
	"publisher": "kaisun",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Education",
		"Visualization",
		"Keymaps",
		"Other"
	],
	"icon": "data/ui/images/logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/kaisunc/vscodevimacademy"
	},
	"homepage": "https://kaisunc.github.io/vscodevimacademy/",
	"galleryBanner": {
		"color": "#222222",
		"theme": "dark"
	},
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "VVA",
					"title": "VVA",
					"icon": "./data/ui/images/activityBarLogo.png"
				}
			]
		},
		"commands": [
			{
				"command": "vva.start",
				"title": "Start Vscode Vim Academy"
			}
		],
		"keybindings": [
			{
				"command": "vva.nextLevel",
				"key": "",
				"when": "editorTextFocus"
			},
			{
				"command": "vva.prevLevel",
				"key": "",
				"when": "editorTextFocus"
			},
			{
				"command": "vva.retryLevel",
				"key": "",
				"when": "editorTextFocus"
			},
			{
				"command": "vva.printTask",
				"key": "",
				"when": "editorTextFocus"
			},
			{
				"command": "vva.debug",
				"key": "",
				"when": "editorTextFocus"
			},
			{
				"command": "vva.toggleCheatSheet",
				"key": "ctrl+alt+c",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"view/title": [],
			"view/item/context": [],
			"editor/title": [],
			"commandPalette": []
		},
		"views": {
			"VVA": [
				{
					"id": "levelSelection",
					"name": "Level Select"
				}
			]
		},
		"configuration": {
			"title": "Vim Academy",
			"properties": {
				"vva.language": {
					"description": "Hints and Tips Language Options",
					"type": "string",
					"default": "en",
					"enum": [
						"en",
						"tw",
						"jp",
						"cn",
						"de",
						"fr",
						"es",
						"kr"
					]
				},
				"vva.taskCount": {
					"description": "Number of tasks per level",
					"type": "number",
					"default": 10
				},
				"vva.trustedDomain": {
					"description": "Added buy site to trusted domain",
					"type": "boolean",
					"default": false
				}
			}
		}
	},
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/chart.js": "^2.9.28",
		"@types/crypto-js": "^4.0.1",
		"@types/css": "0.0.31",
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.61",
		"@types/node-rsa": "^1.0.0",
		"@types/vscode": "^1.51.0",
		"moment": "^2.27.0",
		"tslint": "^5.20.1",
		"typescript": "^3.9.6"
	},
	"dependencies": {
		"@spectrum-css/actionbar": "^3.0.0-beta.3",
		"@spectrum-css/actiongroup": "^1.0.0-beta.2",
		"@spectrum-css/alert": "^3.0.0-beta.3",
		"@spectrum-css/button": "^3.0.0-beta.3",
		"@spectrum-css/dialog": "^3.0.0-beta.3",
		"@spectrum-css/divider": "^1.0.0-beta.2",
		"@spectrum-css/icon": "^2.1.1",
		"@spectrum-css/menu": "^2.1.5",
		"@spectrum-css/page": "^2.0.7",
		"@spectrum-css/popover": "^3.0.0-beta.3",
		"@spectrum-css/progressbar": "^1.0.0-beta.2",
		"@spectrum-css/rating": "^2.0.7",
		"@spectrum-css/rule": "^3.0.0-beta.3",
		"@spectrum-css/slider": "^3.0.0-beta.2",
		"@spectrum-css/steplist": "^3.0.0-beta.2",
		"@spectrum-css/table": "^3.0.0-beta.3",
		"@spectrum-css/textfield": "^3.0.0-beta.5",
		"@spectrum-css/toast": "^3.0.0-beta.3",
		"@spectrum-css/tooltip": "^3.0.0-beta.2",
		"@spectrum-css/treeview": "^3.0.0-beta.3",
		"@spectrum-css/typography": "^2.1.3",
		"@spectrum-css/underlay": "^2.0.7",
		"@spectrum-css/vars": "^2.3.0",
		"chart.js": "^2.9.4",
		"clipboardy": "^2.3.0",
		"crypto-js": "^4.0.0",
		"csstype": "^2.6.11",
		"csvtojson": "^2.0.10",
		"loadicons": "^1.0.0",
		"mini-svg-data-uri": "^1.2.3",
		"node-rsa": "^1.1.1"
	},
	"__metadata": {
		"id": "972f86b8-8745-4712-a8b1-95a66dbb71af",
		"publisherId": "d5dcbfec-b424-47c5-8edf-0196d65b1368",
		"publisherDisplayName": "kaisun",
		"isPreReleaseVersion": false,
		"preRelease": false,
		"installedTimestamp": 1646468188296
	}
}